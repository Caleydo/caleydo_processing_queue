{
  "name": "caleydo_processing_queue",
  "description": "Process long-running tasks in the background using Celery.",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_processing_queue",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_server": "*",
    "caleydo_data_redis": "*"
  },
  "caleydo": {
    "type": "plugin",
    "category": "python_data",
    "plugins": {
      "web": [
        {
          "type": "manager",
          "id": "processing"
        }
      ],
      "python": [
        {
          "type": "namespace",
          "id": "caleydo_processing",
          "file": "processing",
          "namespace": "/api/processing"
        },
        {
          "type": "processing-task",
          "id": "caleydo_processing_task",
          "file": "tasks"
        }
      ]
    },
    "dependencies": {
      "python": {
        "celery[redis,auth,msgpack]": "==4.0.0rc4"
      }
    },
    "scripts": {}
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
